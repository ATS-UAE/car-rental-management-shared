{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"lib": ["ES2015", "dom"],
		"allowSyntheticDefaultImports": true,
		"allowJs": false
	},
	"include": ["utils", "typings"],
	"exclude": [
		"utils/**/*.js",
		"utils/**/*.d.ts",
		"typings/**/*.js",
		"typings/**/*.d.ts"
	]
}
